<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

        <record id="view_kine_demande_traitement_search" model="ir.ui.view">
            <field name="name">kine.demande.traitement</field>
            <field name="model">mz.kine.demande.traitement</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Traitement">
					<field name="patient_id" />
					<field name="traitement_prescris_id" />
					<field name="demande_par" />
					<field name="date_demande" />
					<separator/>
					<filter name="to_day" string="Aujourd'hui" domain="[('date_demande','>=', datetime.datetime.combine(context_today(), datetime.time(0,0,0)))]" />
                    
                </search>
            </field>
        </record>

        <record id="view_kine_demande_traitement_tree" model="ir.ui.view">
            <field name="name">kine.demande.traitement</field>
            <field name="model">mz.kine.demande.traitement</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Traitement">
					<field name="patient_id" />
					<field name="num_fiche" />
					<field name="traitement_prescris_id" />
					<field name="nombre_seances" />
					<field name="observation_medecin" />
					<field name="demande_par" />
					<field name="date_demande" />
                </tree>
            </field>
        </record>
	
		<record id="view_kine_demande_traitement_form" model="ir.ui.view">
            <field name="name">kine.demande.traitement</field>
            <field name="model">mz.kine.demande.traitement</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Traitement">
                    <header>
						<button name="valider" string="Valider" type="object" class="oe_highlight" states="nouveau"/>
						<field name="state" invisible="1" />
				    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="consultation_medical_id" invisible="1" />
                                <field name="patient_id" readonly="1"/>
                                <field name="num_fiche" />
                                <field name="sexe"/>
                                <field name="age"/>
                                <field name="date_naissance"/>
                                <field name="avenue"/>
                                <field name="quartier"/>
                                <field name="commune_id"/>
                                <field name="etat_civile"/>
                                <field name="telephone"/>
                                <field name="contrat"/>
                                <field name="profession_id"/>
                            </group>
                            <group>
                                <field name="traitement_prescris_id" required="1"/>
                                <field name="nombre_seances" required="1"/>
								<field name="observation_medecin" required="1" />
                                <field name="demande_par" readonly="1"/>
                                <field name="date_demande" readonly="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
		<record model="ir.actions.act_window" id="action_demande_traitement_kine">
		    <field name="name">Demande traitement kiné</field>
		    <field name="res_model">mz.kine.demande.traitement</field>
			<field name="view_mode">tree,form</field>
	    </record>


        <record id="view_kine_demande_traitement_serv_kine_tree" model="ir.ui.view">
            <field name="name">kine.demande.traitement</field>
            <field name="model">mz.kine.demande.traitement</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Traitement" decoration-primary="state=='valide'" create="false" default_order='date_demande desc'>
				
					<field name="patient_id" />
					<field name="num_fiche" />
					<field name="traitement_prescris_id" />
					<field name="nombre_seances" />
					<field name="demande_par" />
					<field name="date_demande" />
					<field name="state" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="view_kine_demande_traitement_serv_kine_form" model="ir.ui.view">
            <field name="name">kine.demande.traitement</field>
            <field name="model">mz.kine.demande.traitement</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Traitement">
                    <header>
						<button name="prendre_en_charge" string="Prendre en charge" type="object" class="oe_highlight" states="valide"/>
						<field name="state" invisible="1" />
				    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="consultation_medical_id" invisible="1" />
                                <field name="patient_id" readonly="1"/>
                                <field name="num_fiche" />
                                <field name="sexe"/>
                                <field name="age"/>
                                <field name="date_naissance"/>
                                <field name="avenue"/>
                                <field name="quartier"/>
                                <field name="commune_id"/>
                                <field name="etat_civile"/>
                                <field name="telephone"/>
                                <field name="contrat"/>
                                <field name="profession_id"/>
                            </group>
                            <group>
                                <field name="traitement_prescris_id" required="1"/>
                                <field name="nombre_seances" required="1"/>
								<field name="observation_medecin" required="1" />
                                <field name="demande_par" readonly="1"/>
                                <field name="date_demande" readonly="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_demande_traitement_kine_serv_kine">
		    <field name="name">Demande traitement kiné</field>
		    <field name="res_model">mz.kine.demande.traitement</field>
			<field name="view_mode">tree,form</field>
            <field name="domain"> ['|',('state','=','valide'),('state','=','prise_en_charge')]</field>
	    </record>

        <record model="ir.actions.act_window.view" id="action_demande_traitement_kine_form_view">
		    <field eval="20" name="sequence"/>
		    <field name="view_mode">form</field>
		    <field name="view_id" ref="view_kine_demande_traitement_serv_kine_form"/>
		    <field name="act_window_id" ref="action_demande_traitement_kine_serv_kine"/>
	    </record>

        <record model="ir.actions.act_window.view" id="action_demande_traitement_kine_tree_view">
		    <field eval="10" name="sequence"/>
		    <field name="view_mode">tree</field>
		    <field name="view_id" ref="view_kine_demande_traitement_serv_kine_tree"/>
		    <field name="act_window_id" ref="action_demande_traitement_kine_serv_kine"/>
	    </record>

        <menuitem name="KINESITHERAPIE" id="menu_kine_root"/>

        <menuitem name="Demande traitement" id="menu_demande_traitement_kine" parent="menu_kine_root" action="action_demande_traitement_kine_serv_kine" sequence="1"/>

        <!-- DOSSIER PATIENT KINESITHERAPIE -->
		
		
		
		
		<record model="ir.ui.view" id="view_kine_dossier_patient_search">
			<field name="name">mz.kine.dossier.patient.search</field>
			<field name="model">mz.kine.dossier.patient</field>
			<field name="type">search</field>
			<field name="arch" type="xml">
				<search string="Dossier kine Patient">
					<field name="patient_id" />
					<field name="num_fiche" string="N° Fiche"/>
				</search>
			</field>
		</record>
        
        <record id="view_kine_dossier_patient_tree" model="ir.ui.view">
            <field name="name">kine.dossier.patient</field>
            <field name="model">mz.kine.dossier.patient</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Dossier patient kine">
					<field name="patient_id" />
					<field name="num_fiche" />
					<field name="motif_consultation_id" />
                </tree>
            </field>
        </record>

        <record id="view_kine_dossier_patient_form" model="ir.ui.view">
            <field name="name">kine.dossier.patient</field>
            <field name="model">mz.kine.dossier.patient</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Traitement">
                    <header name="consult">
						<button name="evaluation_deficiences" string="Evaluation déficiences" icon="fa-reply" type="object" class="oe_highlight"/>
						<button name="evaluation_capa_fonctionnelles" string="Evaluation capacités fonctionnelles" icon="fa-male" type="object" class="oe_highlight"/>
						<!-- <field name="state" invisible="1" /> -->
				    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="patient_id" readonly="1"/>
                                <field name="num_fiche" />
                                <field name="sexe"/>
                                <field name="age"/>
                                <field name="date_naissance"/>
                            </group>
                            <group>
                                <field name="avenue"/>
                                <field name="quartier"/>
                                <field name="commune_id"/>
                                <field name="etat_civile"/>
                                <field name="telephone"/>
                                <field name="contrat"/>
                                <field name="profession_id"/>
                            </group>
                        </group>
                        <div class="row">
							<div class="col-lg-12">
								<div class="oe_title">
									<h1>Renseignements medico-technique</h1>
                                    <hr/>
								</div>
							</div>
						</div>
                        <group string="Consultation">
                            <group>
                                <field name="motif_consultation_id" required="1"/>
                                <field name="traitement_en_cours" />
                                <field name="medecin_prescripteur" />
                                <field name="service_prescription" />
                            </group>
                            <group>
                                <field name="diagnostic_medical_id" required="1"/>
                                <field name="prescription_medicale_trait_kine" />
                                <field name="structure_prescription" />
                            </group>
                        </group>

                        <notebook>
                            <page string="Anamnèse">
                                <field name="anamnese" placeholder="Anamnèse" nolabel="1" colspan="4"/>
                            </page>
                            <page string="Antécédents">
                                <group>
                                    <group string="Antécédents medicaux">
                                        <field name="antecedent_medicaux_ids" nolabel="1" colspan="4"/>
                                    </group>
                                    <group string="Antécédents chirurgicaux">
                                        <field name="antecedent_chirurgicaux_ids" nolabel="1" colspan="4"/>
                                    </group>
                                </group>
                                <group>
                                    <group string="Antécédents familiaux">
                                        <field name="antecedent_familiaux_ids" nolabel="1" colspan="4"/>
                                    </group>
                                    <group string="Allergiques">
                                        <field name="allergiques" nolabel="1" colspan="4"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Résultats des examens para cliniques">
                                <field name="resultats_examens_para_cliniques" placeholder="Résultats des examens para cliniques" nolabel="1" colspan="4"/>
                            </page>
                            <page string="Attentes du patient">
                                <field name="attentes_patient" placeholder="Attentes du patient" nolabel="1" colspan="4"/>
                            </page>
                            <page string="Consignes particulières">
                                <field name="consignes_particulieres" placeholder="Consignes particulières" nolabel="1" colspan="4"/>
                            </page>
							<page string="Bilan">
                                <field name="bilan" placeholder="Bilan" nolabel="1" colspan="4"/>
                            </page>
                            <page string="Diagnostic kinésithérapique">
                                <field name="diagnostic_kine" placeholder="Diagnostic kinésithérapique" nolabel="1" colspan="4"/>
                            </page>
                            <page string="Orientation du patient">
                                <group>
                                    <field name="transfert" widget="radio" options="{'horizontal': true}"/>
                                    <field name="lieu_transfert" attrs="{'invisible':[('transfert','!=', 'Oui')], 'required':[('transfert','=', 'Oui')]}"/>
                                    <field name="referencement" widget="radio" options="{'horizontal': true}"/>
                                    <field name="motif_referencement" attrs="{'invisible':[('referencement','!=', 'Oui')], 'required':[('referencement','=', 'Oui')]}"/>
                                    <field name="lieu_referencement" attrs="{'invisible':[('referencement','!=', 'Oui')], 'required':[('referencement','=', 'Oui')]}"/>
                                </group>
                            </page>
                            <!-- <page string="Objectif prise en charge">
                                <group>
                                    <group string="Objectif(s) prise en charge court terme">
                                        <field name="objectif_prise_encharge_court_terme" nolabel="1" colspan="4"/>
                                    </group>
                                    <group string="Objectif(s) prise en charge moyen terme">
                                        <field name="objectif_prise_encharge_moyen_terme" nolabel="1" colspan="4"/>
                                    </group>
                                </group>
                                <group>
                                    <group string="Objectif(s) prise en charge fin traitement">
                                        <field name="objectif_prise_encharge_fin_traitement" nolabel="1" colspan="4"/>
                                    </group>
                                </group>
                            </page> -->
                        </notebook>
                        <div class="row">
							<div class="col-lg-12">
								<div class="oe_title">
									<h1>Directives de traitement</h1>
								</div>
							</div>
						</div>
                        <group>
                            <field name="directives_traitement" placeholder="Directives de traitement" nolabel="1" colspan="4"/>
                        </group>
                        <group string="Objectifs de la prise en charge kinesitherapique">
                            <group>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="oe_title">
                                            <h4>Objectif(s) à court terme</h4>
                                        </div>
                                    </div>
                                </div>
                                <field name="objectif_prise_encharge_court_terme" nolabel="1" colspan="4"/>
                            </group>
                            <group>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="oe_title">
                                            <h4>Objectif(s) à moyen terme</h4>
                                        </div>
                                    </div>
                                </div>
                                <field name="objectif_prise_encharge_moyen_terme" nolabel="1" colspan="4"/>
                            </group>
                            <group>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="oe_title">
                                            <h4>Objectif(s) de fin traitement</h4>
                                        </div>
                                    </div>
                                </div>
                                <field name="objectif_prise_encharge_fin_traitement" nolabel="1" colspan="4"/>
                            </group>
                        </group>
                        <group string="Traitement">
                        </group>
                        <group>
                            <group> 
                                <field name="nombre_seances_prevues" required="1"/>
                            </group>
                        </group>
                        <group>
                            <field name="traitement_line_ids" nolabel="1" colspan="4"/>
                        </group>
                        <group string="Evaluation globale en fin de traitement">
                            <field name="evaluation_globale" nolabel="1" colspan="4"/>
                        </group>
                        <group>
                            <field name="noms_theurapeute"/>
                            <field name="correspondance_prof"/>
                        </group>
                        
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_dossier_patient_kine">
